<fieldset class="nested-fields opening_pickers">
  <div class="form-group">
    <label class="col-sm-1 control-label">Début</label>
    <div class="col-sm-5"><%= f.date_select :starts_at %></div>
    <label class="col-sm-1 control-label">Fin</label>
    <div class="col-sm-5"><%= f.date_select :ends_at %></div>
  </div>
  <div class="form-group mt">
    <label class="col-sm-1 control-label">Durée</label>
    <div class="col-sm-5"><%= f.datetime_select :duration %></div>
    <label class="col-sm-1 control-label">Fréquence</label>
    <div class="col-sm-5"><%= f.datetime_select :frequency %></div>
  </div>
  <div class="text-right mt-lg">
    <%= link_to_remove_association 'Supprimer', f, class: 'btn btn-danger' %>
  </div>
</fieldset>